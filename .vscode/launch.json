  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Examples/ProjectRGBOnLaserScan",
      "type": "debugpy",
      "request": "launch",
      "program": "examples/project_rgb_on_laser_scan.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--data_dir", "~/Programming/scenefun3d/data",
      ]
    },
    {
      "name": "Examples/ShowAnnotations",
      "type": "debugpy",
      "request": "launch",
      "program": "examples/show_annotations.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--data_dir", "~/Programming/scenefun3d/data",
      ]
    },
    {
      "name": "Examples/ExportNerfStudio",
      "type": "debugpy",
      "request": "launch",
      "program": "examples/export_nerfstudio_format.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": [
        "--data_dir", "/data/scenefun3d/data/",
      ]
    },  ]
}

